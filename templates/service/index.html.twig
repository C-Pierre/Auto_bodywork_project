{% extends 'layoutAll.html.twig' %}

{% block main %}
	<div class="body-pack">
		<h1>Nos services et prestations</h1>
		<div class="container-pack">
			{% for service in services %}
				<div class="card-pack">
					<div class="card-pack-top">
						<p>{{ service.name }}</p>
					</div>
					<div class="card-pack-body">
						<p>{{ service.description }}</p>
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
{% endblock %}
