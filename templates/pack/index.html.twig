{% extends 'layoutAll.html.twig' %}

{% block main %}
	<div class="body-pack">
		<h1>Nos forfaits</h1>
		<div class="container-pack">
			{% for pack in packs %}
				<div class="card-pack">
					<div class="card-pack-top">
						<p>{{ pack.name }}</p>
					</div>
					<div class="card-pack-body">
						<p>{{ pack.description }}</p>
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
{% endblock %}
